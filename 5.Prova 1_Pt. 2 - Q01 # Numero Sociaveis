#include <stdio.h>
#define T 5

int somaDivisores(int n){
	int i,soma=0;
	
	for(i=2;i<=n;i++){
		if(n%i == 0)
			soma += n/i;
	}
	
	return soma;
}

int main(){
	int num[T],soma[T],i,cont=0;
	
	printf("Digite a sequencia de numeros: ");
	for(i=0;i<T;i++)
		scanf("%d", &num[i]);
	
	for(i=0;i<T;i++)
		soma[i] = somaDivisores(num[i]);
	
	i = 0;
	while(i < T){
		if(num[i+1] == soma[i])
			cont++;
		i++;
		if(i == T-1)
			if(num[0] == soma[i])
				cont++;
	}
	if(cont == T){
		printf("\nA sequÃªncia de numeros ");
		for(i=0;i<T;i++)
			printf("%d ", num[i]);
		printf("eh sociavel\n");
	}	
	else
		printf("Nao sao sociaveis\n");
	
	return 0;	
}
